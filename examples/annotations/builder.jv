module builder

import build


@build.WithBuilder
class Dog {
  field age, weight, height, breed, sex
  method to_s() {
    return cat(
        'Dog(age=', age, ',weight=', weight, ',height=', height,
        ',breed=', breed, ',sex=', sex, ')')
  }
}


io.println(
    DogBuilder().set_age(5)
        .set_weight(15)
        .set_height(20)
        .set_breed('Poodle')
        .set_sex('Female')
        .build())